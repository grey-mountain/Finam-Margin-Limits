{
    "manifest_version": 3,
    "name": "Finam Margin Limits",
    "description": "Упрощённая таблица маржинальной торговли",
    "version": "1.0",
    "action": {
        "default_icon": "icons/icon.png"
    },
    "background": {
        "service_worker": "scripts/background.js"
    },
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "options_page": "options.html"
}
